{
  "src_folders": ["tests"],
  "output_folder": "reports",
  "page_objects_path": "",
  "globals_path": "",
  "selenium": {
    "start_process": false,
    "server_path": "",
    "log_path": "",
    "host": "hub.lambdatest.com",
    "port": 80,
    "cli_args": {
      "webdriver.chrome.driver": "",
      "webdriver.ie.driver": "",
      "webdriver.firefox.profile": ""
    }
  },
  "test_workers": {
    "enabled": true,
    "workers": "auto"
  },
  "test_settings": {
    "default": {
      "request_timeout_options": {
        "timeout": 1000000
      },
      "launch_url": "https://lambdatest.com",
      "selenium_port": 80,
      "selenium_host": "hub.lambdatest.com",
      "silent": false,
      "screenshots": {
        "enabled": true,
        "path": ""
      },
      "username": "",
      "access_key": "",
      "skip_testcases_on_fail": false,
      "desiredCapabilities": {
        "build": "Nightwatch-Selenium-Sample",
        "visual": true,
        "video": true,
        "console": true,
        "network": true,
        "tunnel": true
      }
    },
    "chrome": {
      "desiredCapabilities": {
        "platform": "Windows 8",
        "browserName": "chrome",
        "version": "83.0",
        "tunnel": true
      }
    },
    "safari": {
      "desiredCapabilities": {
        "platform": "macos 10.13",
        "browserName": "safari",
        "version": "11.0",
        "tunnel": true
      }
    },
    "firefox": {
      "desiredCapabilities": {
        "platform": "Windows 10",
        "browserName": "firefox",
        "version": "70",
        "tunnel": true
      }
    },
    "edge": {
      "desiredCapabilities": {
        "platform": "Windows 10",
        "browserName": "MicrosoftEdge",
        "version": "17.0",
        "tunnel": true
      }
    },
    "ie": {
      "desiredCapabilities": {
        "platform": "Windows 10",
        "browserName": "internet explorer",
        "version": "11",
        "w3c": true,
        "selenium_version": "3.5.2",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "allowBlockedContent": true,
        "ignoreProtectedModeSettings": true,
        "tunnel": true
      }
    }
  }
}
